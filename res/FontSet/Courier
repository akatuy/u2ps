%!PS-Adobe-3.0 Resource-FontSet
/Courier {
	<<
		/notdef { /notdef findfont }
		/base	{ /Courier findfont 1 1 1.2 div scalefontxy }
	>> [
		[ 16#0000 16#303F /base ]
	] makefontset

	/italic <<
		/oblique { /Courier-Oblique findfont 1 1 1.2 div scalefontxy }
	>> [
		[ 16#0000 16#303F /oblique ]
	] addfontsetvariant

	/bold <<
		/bold	{ /Courier-Bold findfont 1 1 1.2 div scalefontxy }
	>> [
		[ 16#0000 16#303F /bold ]
	] addfontsetvariant
} /FontSet defineresource pop
